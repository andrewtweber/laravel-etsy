{
    "name": "andrewtweber/laravel-etsy",
    "type": "library",
    "description": "Etsy marketplace for Laravel",
    "keywords": [
        "laravel",
        "etsy",
        "marketplace"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Weber",
            "homepage": "https://andrew.cool"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/andrewtweber/proste-sdk.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/andrewtweber/slimak.git"
        },
        {
            "type": "composer",
            "url": "https://packagist.org"
        },
        {
            "packagist": false
        }
    ],
    "require": {
        "php": "^8.1",
        "andrewtweber/proste-sdk": "dev-master",
        "andrewtweber/slimak": "dev-master",
        "laravel/framework": "^8.12|^9.0",
        "nesbot/carbon": "^2.60"
    },
    "require-dev": {
        "orchestra/testbench": "^6.17",
        "phpunit/phpunit": "^9"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Etsy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Etsy\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Etsy\\EtsyServiceProvider"
            ]
        }
    }
}
